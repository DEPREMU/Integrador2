{
  "name": "meditime",
  "version": "0.0.0",
  "main": "index.ts",
  "author": "DEPREMU",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "backend": "cd server && npm start",
    "clean": "npx rimraf .expo node_modules package-lock.json server/node_modules server/package-lock.json server/.expo server/dist && npm cache clean --force",
    "clean:all": "npm run clean && npm run install-all",
    "app": "npx expo start -c -g",
    "app-dev": "npx expo start -c -d",
    "build:android": "npm run clean && eas build --clear-cache --platform android --profile preview && npm run install-all",
    "build-dev:android": "npm run clean && eas build --clear-cache --platform android --profile development && npm run install-all",
    "getLocalIP": "node ./getLocalApiUrl.js",
    "install-all": "npm install & cd server & npm install",
    "project": "concurrently \"npm run app\" \"npm run backend\"",
    "start": "npm run getLocalIP & npm run install-all & npm run project",
    "format-all": "npx prettier --write \"src/**/*.{ts,tsx}\" \"server/src/**/*.{ts,tsx}\" \"*.{js,ts,tsx}\" --ignore-path .gitignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@supabase/supabase-js": "^2.51.0",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.9.0",
    "core-js": "^3.44.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "expo": "53.0.20",
    "expo-background-fetch": "~13.1.5",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.1",
    "expo-device": "~7.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-localization": "~16.1.6",
    "expo-notifications": "~0.31.3",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "html2canvas": "^1.4.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.1",
    "memoize-one": "^6.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.0",
    "react-native": "0.79.5",
    "react-native-paper": "^5.14.3",
    "react-native-print": "^0.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "private": true
}
