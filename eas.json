{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "SUPABASE_URL": "$SUPABASE_URL",
        "SUPABASE_KEY": "$SUPABASE_KEY"
      }
    },
    "production": {
      "env": {
        "SUPABASE_URL": "$SUPABASE_URL",
        "SUPABASE_KEY": "$SUPABASE_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
